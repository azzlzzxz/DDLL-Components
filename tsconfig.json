{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@fx/form": ["packages/form/src/index.tsx"],
      "@fx/components": ["packages/components/src/index.tsx"],
      "@fx/table": ["packages/table/src/index.tsx"],
      "@fx/boundary": ["packages/boundary/src/index.tsx"]
    }
  },
  "include": [".dumi/**/*", ".dumirc.ts", "src/**/*"]
}
